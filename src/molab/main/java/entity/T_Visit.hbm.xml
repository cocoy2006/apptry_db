<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="molab.main.java.entity">
	<class lazy="false" name="T_Visit" table="T_Visit">
		<id name="id" type="java.lang.Integer">
			<column length="8" name="ID" />
			<generator class="identity"></generator>
		</id>
		<property name="application_id" type="java.lang.Integer">
			<column name="APPLICATION_ID" />
		</property>
		<property name="ipAddress" type="java.lang.String">
			<column name="IPADDRESS" />
		</property>
		<property name="macAddress" type="java.lang.String">
			<column name="MACADDRESS" />
		</property>
		<property name="fromUrl" type="java.lang.String">
			<column name="FROMURL" />
		</property>
		<property name="loginTime" type="java.lang.Long">
			<column name="LOGINTIME" />
		</property>
		<property name="durationTime" type="java.lang.Long">
			<column name="DURATIONTIME" />
		</property>
		<property name="userAgent" type="java.lang.String">
			<column name="USERAGENT" />
		</property>
	</class>
</hibernate-mapping>
