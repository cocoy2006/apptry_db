<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="molab.main.java.entity">
	<class lazy="false" name="T_Deployment" table="T_Deployment">
		<id name="id" type="java.lang.Integer">
			<column length="8" name="ID" />
			<generator class="identity"></generator>
		</id>
		<property name="dispatcher_id" type="java.lang.Integer">
			<column name="DISPATCHER_ID" />
		</property>
		<property name="application_id" type="java.lang.Integer">
			<column name="APPLICATION_ID" />
		</property>
		<property name="emulator_id" type="java.lang.Integer">
			<column name="EMULATOR_ID" />
		</property>
		<property name="state" type="java.lang.String">
			<column name="STATE" />
		</property>
	</class>
</hibernate-mapping>
