<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="molab.main.java.entity">
	<class lazy="false" name="T_Application" table="T_Application">
		<id name="id" type="java.lang.Integer">
			<column length="8" name="ID" />
			<generator class="identity"></generator>
		</id>
		<property name="md5" type="java.lang.String">
			<column name="MD5" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<property name="aliasName" type="java.lang.String">
			<column name="ALIASNAME" />
		</property>
		<property name="size" type="java.lang.Long">
			<column name="SIZE" />
		</property>
		<property name="packageName" type="java.lang.String">
			<column name="PACKAGENAME" />
		</property>
		<property name="version" type="java.lang.String">
			<column name="VERSION" />
		</property>
		<property name="os" type="java.lang.String">
			<column name="OS" />
		</property>
		<property name="startActivity" type="java.lang.String">
			<column name="STARTACTIVITY" />
		</property>
		<property name="icon" type="java.sql.Blob">
			<column name="ICON" />
		</property>
	</class>
</hibernate-mapping>
