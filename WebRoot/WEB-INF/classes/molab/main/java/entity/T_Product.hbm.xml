<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="molab.main.java.entity">
	<class lazy="false" name="T_Product" table="T_Product">
		<id name="id" type="java.lang.Integer">
			<column length="8" name="ID" />
			<generator class="identity"></generator>
		</id>
		<property name="amount" type="java.lang.Integer">
			<column name="AMOUNT" />
		</property>
		<property name="currency" type="java.lang.String">
			<column name="CURRENCY" />
		</property>
		<property name="clicks" type="java.lang.Integer">
			<column name="CLICKS" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<property name="category" type="java.lang.String">
			<column name="CATEGORY" />
		</property>
		<property name="description" type="java.lang.String">
			<column name="DESCRIPTION" />
		</property>
		<property name="state" type="java.lang.Integer">
			<column name="STATE" />
		</property>		
	</class>
</hibernate-mapping>
